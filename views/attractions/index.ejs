    <% layout('layouts/boilerplate')%> 
    <div id="cluster-map"></div>
    <h1>All Attractions</h1>
    <div>
        <a href="/attractions/new">Add Attraction</a>
    </div>
        <% for(let attraction of attractions) { %>
            <div class="card mb-3">
                <div class="row">
                    <div class="col-xl-4">
                        <% if(attraction.images.length) { %> 
                        <img class="img-fluid w-100 h-100" src="<%= attraction.images[0].url %>" alt="" crossorigin>
                        <% }else { %> 
                        <img class="img-fluid" src="https://res.cloudinary.com/paulomllanos/image/upload/v1659716147/ChileTour/bal5pcsbefqbgkxcjfaz.jpg" alt="" crossorigin>
                        <% } %> 
                    </div>
                    <div class="col-md-8">
                        <div class="card-body">
                            <h5 class="card-title"><%= attraction.title %></h5>
                            <p class="card-text"><%= attraction.description %></p>
                            <p class="card-text">
                                <small class="text-muted"><%= attraction.location %></small>
                            </p>
                            <a class="btn btn-primary" href="/attractions/<%= attraction._id%>">View <%= attraction.title %></a>
                        </div>
                    </div>
                </div>
            </div>
        <% } %> 

 <script>
    const mapToken = '<%-process.env.MAPBOX_TOKEN%>';
    const attractions = {features: <%- JSON.stringify(attractions) %>};
</script>

<script src="/javascripts/clusterMap.js"></script>